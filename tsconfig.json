{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "experimentalDecorators": true,
    "module": "commonjs",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "downlevelIteration": true
  }
}
